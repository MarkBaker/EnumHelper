{
  "name": "markbaker/enumhelper",
  "description": "A small library that provides Helper Traits for PHP 8.1 Enums",
  "keywords": ["enum", "helper", "trait"],
  "homepage": "https://github.com/MarkBaker/EnumHelper",
  "license": "MIT",
  "authors": [
    {
      "name": "Mark Baker",
      "email": "mark@demon-angel.eu"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.4",
    "phpunit/phpunit": "^9.5",
    "phpcompatibility/php-compatibility": "^9.0",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0"
  },
  "autoload": {
    "psr-4": {
      "EnumHelper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EnumHelper\\TestData\\": "tests/data",
      "EnumHelper\\Tests\\": "tests/src"
    }
  },
  "scripts": {
    "style": "phpcs --report-width=200 --standard=PSR2 --report=summary,full src/ tests/ -n",
    "versions": "phpcs --report-width=200 --standard=PHPCompatibility --report=summary,full classes/src/ --runtime-set testVersion 8.1- -n"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}